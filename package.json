{
  "name": "generate-svelte-app",
  "description": "Quick way to start your svelte app",
  "version": "1.0.0",
  "author": "Raul de Melo @raulfdm",
  "bin": {
    "generate-svelte-app": "./bin/run"
  },
  "bugs": "https://github.com/raulfdm/generate-svelte-app/issues",
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [

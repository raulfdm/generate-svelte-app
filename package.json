    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "tslib": "^1"
  },
  "devDependencies": {
    "@types/node": "^10",
    "husky": "^3.0.9",
    "prettier": "^1.18.2",
    "pretty-quick": "^2.0.0",
    "ts-node": "^8",
    "typescript": "^3.3"
  },
  "husky": {
    "hooks": {
